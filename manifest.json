{
  "manifest_version": 2,
  "name": "ChatGPT++",
  "version": "1.0",
  "description": "A simple extension that adds a button to the Chrome toolbar.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/chat/*"],
      "js": ["jquery.min.js", "content-script.js"]
    }
  ]
}
